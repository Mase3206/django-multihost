{
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "runserver",
			"type": "debugpy",
			"request": "launch",
			"args": [
				"runserver"
			],
			"django": true,
			"autoStartBrowser": false,
			"cwd": "${workspaceFolder}",
			"module": "manage"
		}
	]
}